<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.myservice" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".service.MyStartService">

        </service>
        <activity android:name=".MySecActivity">

        </activity>
        <service android:name=".service.MyBindService">

        </service>
        <service android:name=".service.MyServer">
            <intent-filter>
                <action android:name="com.example.asus.service.MyServer"></action>
            </intent-filter>
        </service>
    </application>

</manifest>
